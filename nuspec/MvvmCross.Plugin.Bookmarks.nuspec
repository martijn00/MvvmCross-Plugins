<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>MvvmCross.Plugin.Bookmarks</id>
		<version>4.1.4</version>
		<title>MvvmCross - Bookmarks Plugin</title>
		<authors>MvvmCross</authors>
		<owners>Stuart Lodge</owners>
		<licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
		<projectUrl>https://github.com/MvvmCross/MvvmCross-Plugins</projectUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>MvvmCross is the .NET MVVM framework for cross-platform solutions, including Xamarin iOS, Xamarin Android, Xamarin Forms, Windows and Mac.

This package contains the 'Bookmarks' plugin for MvvmCross</description>
		<tags>mvvm mvvmcross cross xamarin android ios forms monodroid monotouch xamarin.android xamarin.ios xamarin.forms wpf windows8 winrt net net45 netcore wp wpdev windowsphone windowsstore uwp plugin</tags>
		<iconUrl>http://i.imgur.com/BvdAtgT.png</iconUrl>
		<dependencies>
			<group targetFramework="net">
				<dependency id="MvvmCross.Platform" version="4.1.4" />
			</group>
			<group targetFramework="win">
				<dependency id="MvvmCross.Platform" version="4.1.4" />
			</group>
			<group targetFramework="wp">
				<dependency id="MvvmCross.Platform" version="4.1.4" />
			</group>
			<group targetFramework="wpa">
				<dependency id="MvvmCross.Platform" version="4.1.4" />
			</group>
			<group targetFramework="MonoAndroid">
				<dependency id="MvvmCross.Platform" version="4.1.4" />
			</group>
			<group targetFramework="Xamarin.Mac20">
				<dependency id="MvvmCross.Platform" version="4.1.4" />
			</group>
			<group targetFramework="Xamarin.iOS10">
				<dependency id="MvvmCross.Platform" version="4.1.4" />
			</group>
			<group targetFramework="portable-net45+win8+wpa81+wp8">
				<dependency id="MvvmCross.Platform" version="4.1.4" />
			</group>
			<group targetFramework="uap">
				<dependency id="MvvmCross.Platform" version="4.1.4" />
			</group>
			<group targetFramework="dotnet">
				<dependency id="MvvmCross.Platform" version="4.1.4" />
			</group>
		</dependencies>
	</metadata>
	<files>		
		<!-- PCL -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Bookmarks.*" target="lib\portable-net45+win+wpa81+wp80" />

		<!-- dotnet -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Bookmarks.*" target="lib\dotnet" />

		<!-- src -->
		<file src="..\Bookmarks\MvvmCross.Plugins.Bookmarks\**\*.cs" 
			target="src\MvvmCross.Plugins.Bookmarks" />

		<!-- phone -->
		<file src="..\bin\Release\Mvx\Portable\MvvmCross.Plugins.Bookmarks.*" target="lib\wp8" />
		<file src="..\bin\Release\Mvx\WindowsPhone\MvvmCross.Plugins.Bookmarks.WindowsPhone.*" target="lib\wp8" />
		<file src="..\Bookmarks\MvvmCross.Plugins.Bookmarks.WindowsPhone\**\*.cs" 
			target="src\MvvmCross.Plugins.Bookmarks.WindowsPhone" />
		<file src="BootstrapContent\BookmarksPluginBootstrap.cs.pp" target="content\wp8\Bootstrap\BookmarksPluginBootstrap.cs.pp" />

	</files>
</package>









